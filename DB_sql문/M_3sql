--event_key 키값 시퀀스
drop SEQUENCE event_seqid;

create SEQUENCE event_seqid
start with 1
increment by 1;

--insert into event values('EV' || TO_CHAR(event_seqid.nextval),'썸네일','내용이미지','2024-01-01','2024-12-31','제목','내용');
?--'EV' || TO_CHAR(event_seqid.nextval)
 -------------------------------------------------------------------------
